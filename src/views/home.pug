extends base
    
block content
    header=pageTitle
    table#position
        tr  
            th ⭐️이름⭐️
            th 밥 🍚
            th 책 📖
            th 연필 ✏️
            th 박수👏🏻
            th 하트 ❤️
            th ⭐️합계⭐️ 
        each value in notionData
            tr
                th=value.name
                td(data-id=value.id data-name="food")=value.food
                    //- a(href="/" data-id=value.id )#btnFood=value.food
                td(data-id=value.id data-name="book")=value.book
                    //- a(href="/" data-id=value.id )#btnBook=value.book
                td(data-id=value.id data-name="pencil")=value.pencil
                td(data-id=value.id data-name="good")=value.good
                td(data-id=value.id data-name="heart")=value.heart
                th(data-id=value.id )=value.total